<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Dream It Get It Application</display-name>
    <description>
        Application using artificial intelligence for filtering the candidates and helps in getting the right candidates for you.
    </description>

    <servlet>
        <servlet-name>dreamitgetit</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>dreamitgetit</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
</web-app>
